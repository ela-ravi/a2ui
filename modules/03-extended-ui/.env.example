# LLM Provider Configuration
# Set to "true" to enable a provider in the UI
# Users will enter their own API keys in the settings panel

# Ollama (local, dev only)
VITE_OLLAMA_ENABLED=true

# Cloud Providers - enable the ones you want available
VITE_OPENAI_ENABLED=true
VITE_ANTHROPIC_ENABLED=true
VITE_GEMINI_ENABLED=true
VITE_OPENROUTER_ENABLED=true

# HuggingFace - disabled by default (no browser CORS support)
VITE_HUGGINGFACE_ENABLED=false
